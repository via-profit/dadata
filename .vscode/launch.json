  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to debugger",
      "type": "node",
      "request": "attach",
      "stopOnEntry": false,
      "protocol": "inspector",
      "port": 9229,
      "sourceMaps": true,
      "restart": true,
      "outFiles": [
        "${workspaceRoot}/build/index.js"
      ],
      "localRoot": "${workspaceRoot}/src/playground/index.ts",
      "remoteRoot": "${workspaceRoot}/build/index.js",
      "sourceMapPathOverrides": {
        "webpack://@via-profit/dadata/./*": "${workspaceRoot}/*"
      },
      "trace": "verbose"
    },
  ]
}